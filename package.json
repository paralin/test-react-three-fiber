{
	"name": "spacegame",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"prettier": "^3.0.0",
		"turbo": "latest"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"packageManager": "yarn@1.22.21"
}
